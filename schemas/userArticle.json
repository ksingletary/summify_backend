{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/userArticle.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "articleTitle": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255
      },
      "articleUrl": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255,
        "format": "uri"
      },
      "summary": {
        "type": "string",
        "minLength": 1
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "articleTitle",
      "articleUrl",
      "summary"
    ]
  }
  